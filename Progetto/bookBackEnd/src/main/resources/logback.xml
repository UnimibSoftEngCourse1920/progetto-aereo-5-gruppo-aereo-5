<?xml version="1.0" encoding="UTF-8"?>

<configuration>

<appender name="consoleAppender" class="ch.qos.Logback.core.ConsoleAppender">
	<encoder>
		<Pattern>
			%-5level %msg %d{HH:mm:ss.SSS} %logger{36} %n 
		</Pattern>
	</encoder>
</appender>
<logger name="gruppoaereo5" level="DEBUG" additivity="false">
	<appender-ref ref="coleAppender"/>
</logger>
<root level="INFO">
	<appender-ref ref="coleAppender"/>
</root>
</configuration>